extends ../layout/index

block content
	.auth
		h4 Авторизация
		form(id="formAuth" class="auth__form" action='./auth', method='POST' enctype="multipart/form-data")
			input(type="text" name="login" placeholder="Логин" value="name")
			input(type="password" name="pass" placeholder="Пароль" value="pass")
			button(type="submit") sub